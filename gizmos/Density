  ypos 384
 }
push $Nb7d4f000
 Expression {
  channel0 alpha
  expr0 1-exp(-a)
  name Density2Alpha1
  xpos 402
  ypos 466
 }
 Switch {
  inputs 3
  which {{parent.output}}
  name Switch1
  xpos 559
  ypos 533
 }
