  name Unpremult
  xpos 402
  ypos 72
  disable {{!parent._unpremult_}}
 }
 Dot {
  name Dot3
  xpos 436
  ypos 210
 }
set Nb7da4800 [stack 0]
 Dot {
  name Dot1
  xpos 751
  ypos 210
 }
 Multiply {
  channels alpha
  value {{parent.Mult_5.value}}
  name Mult_1
  label "x\[value value]"
  xpos 717
  ypos 366
 }
 Expression {
  channel0 alpha
  expr0 1-exp(-a)
  name Density2Alpha2
  xpos 717
  ypos 472
 }
push $Nb7da4800
 Expression {
  temp_name0 Vdmax
  temp_expr0 densityMax
  channel0 alpha
  expr0 a<1?-log(1-a):Vdmax
  name Alpha2Density1
  xpos 402
  ypos 301
 }
 Multiply {
  channels alpha
  name Mult_5
  label "x\[value value]"
  xpos 402
  ypos 368
 }
set Nb7d4f000 [stack 0]
